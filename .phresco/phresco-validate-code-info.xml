<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
      <goal>validate-code</goal>
      <description>Goal which validates the Html-5 Archetype project</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
	  <configuration>
      <parameters>
        <parameter>
			<name>
                <value lang="en">Validate Against</value>
            </name>
			<type>List</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>sonar</key>
			<possibleValues>
				<value key="src" dependency="src,skipTests,showSettings,environmentName,webservices">Source</value>
				<value key="functional">Functional Test</value>
			</possibleValues>
			<multiple>false</multiple>
			<sort>false</sort>
			<show>true</show>
		  <value></value>
        </parameter>
		<parameter>
		<pluginParameter>plugin</pluginParameter>
			<name>
                <value lang="en">Technology</value>
            </name>
			<mavenCommands>
				<mavenCommand>
					<key>js</key>
					<value>-Pjs -DskipTests -Dsonar.phase=test</value>
				</mavenCommand>
				<mavenCommand>
					<key>java</key>
					<value>-Pjava</value>
				</mavenCommand>
				<mavenCommand>
					<key>html</key>
					<value>-Phtml</value>
				</mavenCommand>
				<mavenCommand>
					<key>web</key>
					<value>-Pweb</value>
				</mavenCommand>
			</mavenCommands>
          <type>List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
		  <key>src</key>
			<possibleValues>
				<value key="js" dependency="showSettings,environmentName,webservices,skipTests">Js</value>
				<value key="java" dependency="skipTests">Java</value>
				<value key="html">Html</value>
				<value key="web">Jsp/Jsf</value>
			</possibleValues>
			<multiple>false</multiple>
			<sort>false</sort>
			<show>false</show>
		  <value></value>
        </parameter>
	  <parameter>
			<pluginParameter>plugin</pluginParameter>
			<mavenCommands>
				<mavenCommand>
					<key>true</key>
					<value>-DskipTests=true -Dmaven.test.skip=true</value>
				</mavenCommand>
				<mavenCommand>
					<key>false</key>
					<value>-Dmaven.test.failure.ignore=true</value>
				</mavenCommand>
			</mavenCommands>
			<name>
				<value lang="en">Skip Unit Test</value>
			</name>
			<type>Boolean</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>skipTests</key>
			<multiple>false</multiple>
			<sort>false</sort>
			<show>false</show>
			<value>false</value>
      </parameter>
	   <parameter>
			<name>
				<value lang="en">Show Settings</value>
			</name>
			<type>Boolean</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>showSettings</key>
			<multiple>false</multiple>
			<value></value>
			<sort>false</sort>
			<show>false</show>
			<dependency>environmentName</dependency>
		</parameter>
	   <parameter>
			<name>
				<value lang="en">Environment</value>
			</name>
			<type>DynamicParameter</type>
			<dynamicParameter>
				<class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
				<dependencies>
					<dependency>
						<groupId>com.photon.phresco.commons</groupId>
						<artifactId>phresco-commons</artifactId>
						<type>jar</type>
						<version>3.2.1.3002-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</dynamicParameter>
			<required>true</required>
			<editable>true</editable>
			<key>environmentName</key>
			<multiple>false</multiple>
			<value></value>
			<sort>false</sort>
			<show>true</show>
			<dependency>webservices</dependency>
		</parameter>
		<parameter>
			<pluginParameter>plugin</pluginParameter>
			<mavenCommands>
				<mavenCommand>
					<key>showErrors</key>
					<value>-e</value>
				</mavenCommand>
			</mavenCommands>
			<name>
				<value lang="en">WebServices</value>
			</name>
			<type>DynamicParameter</type>
			<dynamicParameter>
				<class>com.photon.phresco.framework.param.impl.EnvironmentWebservicesImpl</class>
				<dependencies>
					<dependency>
						<groupId>com.photon.phresco.framework</groupId>
						<artifactId>phresco-framework-impl</artifactId>
						<type>jar</type>
						<version>3.2.1.3002-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</dynamicParameter>
			<required>true</required>
			<editable>true</editable>
			<key>webservices</key>
			<multiple>false</multiple>
			<value></value>
			<sort>false</sort>
			<show>true</show>
		</parameter>
		<parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
            </parameter>
	  </parameters>
      </configuration>
    </mojo>
</mojos>